#!/bin/sh

dir="/data/adb/beszel"
source ${dir}/setting
echo $PORT
echo $KEY

rm $dir/log.txt 2>/dev/null
touch $dir/log.txt
if ! (pgrep -f "beszel.*verbose") > /dev/null ; then
  source ${dir}/setting
  nohup "$dir/beszel_agent" > "$dir/log.txt" 2>&1 &
  echo "beszel_agent has started"
else
  echo "beszel_agent is running"
fi